# Redis settings (not required for docker-compose)
REDIS_HOST=localhost
# REDIS_PASSWORD=
# REDIS_PORT=6379
# REDIS_DB=0

# Azure OpenAI API settings
AZURE_OPENAI_API_KEY=your_api_key_here

# Optional: Override default Azure OpenAI settings
# AZURE_OPENAI__DEPLOYMENT_NAME=your_deployment_name
# AZURE_OPENAI__AZURE_ENDPOINT=your_endpoint_url
# AZURE_OPENAI__API_VERSION=your_api_version
# AZURE_OPENAI__TEMPERATURE=0.7

# Optional: Override default pipeline settings
# PIPELINE__NUM_EXAMPLES=5
# PIPELINE__BATCH_SIZE=12
# PIPELINE__OUTPUT_FILE=ads.json
# PIPELINE__FIXED_OUTPUT_FILE=fixed_ads.json
# PIPELINE__FINAL_OUTPUT_FILE=fixed_ads_list.json
# PIPELINE__RETRY_ATTEMPTS=3
# PIPELINE__RETRY_DELAY=60
